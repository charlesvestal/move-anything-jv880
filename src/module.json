{
  "id": "minijv",
  "name": "Mini-JV",
  "abbrev": "JV",
  "version": "0.4.2",
  "description": "ROM-based PCM rompler emulator",
  "author": "nukeykt/giulioz (port: charlesvestal)",
  "license": "Non-commercial",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "raw_ui": true,
  "raw_midi": true,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": false,
    "midi_channels": 16,
    "chainable": true,
    "component_type": "sound_generator",
    "default_forward_channel": 1,
    "chain_params": [
      {
        "key": "mode",
        "name": "Mode",
        "type": "enum",
        "options": ["Patch", "Performance"],
        "default": 0
      },
      {
        "key": "preset",
        "name": "Preset",
        "type": "int",
        "min": 0,
        "max_param": "preset_count",
        "default": 0
      },
      {
        "key": "performance",
        "name": "Performance",
        "type": "int",
        "min": 0,
        "max": 47,
        "default": 0
      },
      {
        "key": "octave_transpose",
        "name": "Octave",
        "type": "int",
        "min": -4,
        "max": 4,
        "default": 0
      }
    ]
  }
}
