{
  "id": "minijv",
  "name": "Mini-JV",
  "abbrev": "JV",
  "version": "0.4.8",
  "description": "ROM-based PCM rompler emulator",
  "author": "nukeykt/giulioz (port: charlesvestal)",
  "license": "Non-commercial",
  "ui": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "raw_ui": true,
  "raw_midi": true,
  "standalone": false,
  "assets": {
    "path": "roms",
    "label": "ROMs",
    "extensions": [
      ".bin",
      ".rom"
    ],
    "allowFolders": true,
    "description": "Requires JV-880 ROM files. Upload the rom1.bin and rom2.bin files, or a folder containing them.",
    "hint": "Upload JV-880 ROM files. The plugin requires the v1.0.0 waveform ROMs including jv880_nvram.bin. Upload expansion bins to the expansions folder.",
    "hint_url": "https://github.com/charlesvestal/move-anything-jv880",
    "hint_url_label": "Read more"
  },
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": false,
    "midi_channels": 16,
    "chainable": true,
    "component_type": "sound_generator",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "Mini-JV",
          "params": [
            "mode",
            "preset",
            "performance",
            "octave_transpose"
          ],
          "knobs": [
            "preset",
            "octave_transpose"
          ]
        }
      }
    },
    "default_forward_channel": 1,
    "chain_params": [
      {
        "key": "mode",
        "name": "Mode",
        "type": "enum",
        "options": [
          "Patch",
          "Performance"
        ],
        "default": 0
      },
      {
        "key": "preset",
        "name": "Preset",
        "type": "int",
        "min": 0,
        "max_param": "preset_count",
        "default": 0
      },
      {
        "key": "performance",
        "name": "Performance",
        "type": "int",
        "min": 0,
        "max": 47,
        "default": 0
      },
      {
        "key": "octave_transpose",
        "name": "Octave",
        "type": "int",
        "min": -4,
        "max": 4,
        "default": 0
      }
    ]
  }
}
